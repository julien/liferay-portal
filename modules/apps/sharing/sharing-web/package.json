{
	"dependencies": {
		"clay-alert": "2.16.0",
		"clay-button": "2.16.0",
		"clay-multi-select": "2.16.0",
		"clay-select": "2.16.0",
		"clay-sticker": "2.16.0",
		"frontend-js-web": "^4.0.0",
		"metal-component": "2.16.8",
		"metal-soy": "2.16.8"
	},
	"jest": {
		"globals": {
			"Liferay": {}
		},
		"moduleNameMapper": {
			"(.*)\\.soy": "$1.soy.js"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/test/setup/index.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"<rootDir>/test/setup/",
			"<rootDir>/node_modules/"
		],
		"transformIgnorePatterns": [
			"<rootDir>.*(node_modules)(?!.*metal.*).*$"
		]
	},
	"name": "sharing-web",
	"private": true,
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts check",
		"format": "liferay-npm-scripts fix",
		"test": "liferay-npm-scripts test"
	},
	"version": "2.0.0"
}
